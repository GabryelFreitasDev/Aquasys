
<Shell
    x:Class="Aquasys.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
    xmlns:local="clr-namespace:Aquasys.MVVM.Views"
    mc:Ignorable="d"
    Shell.NavBarIsVisible="True"
    Shell.FlyoutBehavior="Flyout"
    Shell.TabBarIsVisible="False"
    Shell.BackgroundColor="{StaticResource Primary}"
    Shell.TitleColor="White">

    <Shell.Behaviors>
        <toolkit:StatusBarBehavior StatusBarColor="{StaticResource Transparent}"
                               StatusBarStyle="LightContent" />
    </Shell.Behaviors>

    <ShellContent ContentTemplate="{DataTemplate local:Vessel.VesselListPage}"/>
</Shell>